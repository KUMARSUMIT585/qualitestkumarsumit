<?xml version="1.0" encoding="utf-8" ?>  
<configuration>  
    <startup>   
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />  
    </startup>  
    <appSettings>  
    <!-- Eligible values for Browser Names :   CHROME,IE-->
    <add key="BrowserName" value="FF"/>  
    <add key="BaseUrl" value="http://automationpractice.com"/> 
    <!--In case we want to change paths for Drivers, change below path-->
    <!--add key="DriverPath" value="C://Program Files (x86)/Jenkins/workspace/vtaf/Utilities/Drivers"/--> 
    <add key="DriverPath" value="./Utilities/Drivers"/> 
    
    <add key="Environment" value="QA"/>
    <add key="AppUnderTest" value="Shopping Portal"/>
    <add key="BaseURI" value="http://localhost:8085"/>
    <!--In case we want to change test reports folder, change below path-->
    <!--add key="ReportsPath" value="C://Program Files (x86)/Jenkins/workspace/vtaf/Reports" /-->
    
    <add key="ReportsPath" value="./Reports" />
    <!--In order to clean up Reports folder select YES else NO-->
    <add key="ReportsCleanUp" value="NO"/>
  </appSettings>  
</configuration>